{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["package.json", "README.md", "lib/src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|tests/)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false
    }
  },
  "opts": {
    "destination": "./docs/",
    "readme": "./README.md",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "dark",
      "title": "<img src='https://cdn.discordapp.com/attachments/730165562189152407/870362596459569172/DAR.png' class='DAR_logo'/>",
      "meta" : [
        {
          "name": "author",
          "content": "discord-antiraid"
        },
        {
          "name": "description",
          "content": "Discord.js extention library for protect discord servers against Raids"
        },
        {
          "name": "og:image",
          "content": "https://cdn.discordapp.com/attachments/730165562189152407/870362596459569172/DAR.png"
        }
      ],
      "menu": [
        {
          "title": "Github",
          "link": "https://github.com/Derpinou/discord-antiraid",
          "target": "_blank",
          "class": "some-class",
          "id": "some-id"
        }
      ],
      "footer": "Made with ‚ù§ By Derp",
      "create_style": "nav{background: yellow} footer {background: #5865F2} body {background: #2c2f33} .pre-top-bar-container {background: #5865F2} .prettyprint.source, .prettyprint code {background-color: #23272A} .typ {color: #F6F6F6} .type-signature {color: #5865F2}"
    }
  }
}